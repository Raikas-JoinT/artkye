  <% if @user.image? %>
    <%= image_tag @user.image.url %>
  <% else %>
    <%= image_tag("https://krwsjapan.conohawing.com/wp-content/uploads/2020/11/no-image.jpg") %>
  <% end %>

<%= form_with model: @user, local: true do |f| %>
  <%= f.file_field :image %>
  <%= f.submit 'プロフィール編集を完了する' %>
<% end %>